	<settings>
		<setting name="jdbcTypeForNull" value="NULL"/>
	</settings>

	<typeAliases>
		<typeAlias type="com.kh.samsam.member.model.vo.Member" alias="member"/>
		<typeAlias type="com.kh.samsam.board.model.vo.NoticeFaq" alias="noticeFaq"/>
		<typeAlias type="com.kh.samsam.board.model.vo.Qna" alias="qna"/>
